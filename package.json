    "eslint": "7.18.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@types/chai": "4.2.14",
    "@types/cucumber": "6.0.1",
    "axios": "0.21.1",
    "chai": "4.2.0",
    "cucumber": "6.0.5",
    "cucumber-html-reporter": "5.2.0",
    "cucumber-pretty": "6.0.0",
    "log4js": "6.3.0"
  }
}
